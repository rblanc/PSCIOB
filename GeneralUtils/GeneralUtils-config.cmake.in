SET (GENERALUTILS_VERSION_STRING @GeneralUtils_VERSION@)
SET (GENERALUTILS_VERSION_MAJOR  @GeneralUtils_MAJOR_VERSION@)
SET (GENERALUTILS_VERSION_MINOR  @GeneralUtils_MINOR_VERSION@)

SET(GENERALUTILS_SUBDIRS 3rdParty/clipper_ver4.7.3/cpp BoundingBoxesUtils GeneralUtils ITKUtils ITKVTKUtils LinearTransformUtils RandomVariables VTKClipperWrapper VTKUtils)

SET(GENERALUTILS_DIR @CMAKE_INSTALL_PREFIX@)

SET(GENERALUTILS_INCLUDE_DIRS  @CMAKE_INSTALL_PREFIX@/include/GeneralUtils)
foreach(dir ${GENERALUTILS_SUBDIRS})
	SET(curdir @CMAKE_INSTALL_PREFIX@/include/GeneralUtils/${dir})
	LIST(APPEND GENERALUTILS_INCLUDE_DIRS ${curdir})
endforeach(dir)

SET(GENERALUTILS_LIBRARY_DIR @CMAKE_INSTALL_PREFIX@/lib/)
SET(GENERALUTILS_LIBRARIES GeneralUtils) 
